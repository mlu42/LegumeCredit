<?xml version="1.0" encoding="utf-8"?>
<shape
android:shape="rectangle"
xmlns:android="http://schemas.android.com/apk/res/android">
	<solid
    android:color="@color/text_green"/>

    <corners
    android:topLeftRadius="5dp"
    android:bottomLeftRadius="5dp"/>



    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp"
        />
    <stroke
        android:width="3dp"
        android:color="@color/text_green"
        android:dashGap="1dp"
        />

</shape>